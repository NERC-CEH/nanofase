&allocatable_array_sizes
default_fractional_comp_size = 4
default_np_forms = 4
default_np_extra_states = 2
n_soil_layers = 3
n_other_sites = 1
/

&calibrate
calibration_run = .false.
site_data = ""
start_site = ""
end_site = ""
other_sites = ""
/

&data
input_file = "data/thames/data.nc"
flat_input = "data/thames/flat_tio2_2000.nc"
constants_file = "data/thames/constants_tio2.nml"
output_file = "output.csv"
output_path = "data/output/"
/

&run
timestep = 86400
n_timesteps = 366
start_date = "2000-01-01"
epsilon = 1e-50,
error_output = .false.
log_file_path = "log/"
/

&global
default_fractional_comp = 30, 30, 30, 10
warm_up_period = 0
nanomaterial_density = 4320
/

&soil
soil_layer_depth = 0.05, 0.15, 0.2
default_k_att = 3.65d-3
include_bioturbation = .true.
include_attachment = .true.
/

&river
max_river_reaches = 100
default_meandering_factor = 1.0
default_water_temperature = 10
default_alpha_hetero = 0.01
default_alpha_hetero_estuary = 0.01
include_bed_sediment = .true.
/

&sources
include_point_sources = .true.
/
