&data
inputFile = "data/data-gwava.nc",
outputFile = "data/output.csv"
/

&run
timeStep = 86400,
nTimeSteps = 1095,
epsilon = 1e-10,
errorOutput = .false.
defaultGridSize = 5000
/

&global
default_grid_size = 5000,
default_distribution_sediment = [ 50, 30, 10, 7, 3 ],
default_distribution_np = [ 50, 30, 10, 7, 3 ]
/

&soil
defaultSoilLayerDepth = 0.1
/

&river
maxRiverReaches = 100
defaultMeanderingFactor = 1.0
defaultWaterTemperature = 10
default_alpha_hetero = 0.01
/
